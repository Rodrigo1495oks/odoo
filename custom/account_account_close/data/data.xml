<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate='1'> <!-- para que no se reinicie la secuencia-->
        <!-- sequences for Fiscal year -->
        <record id='seq_fiscal_year' model='ir.sequence'>
            <field name='name'>Fiscal Year</field>
            <field name='code'>account.fiscal.year</field>
            <field name='prefix'>FY</field>
            <field name='suffix'>s</field>
            <field name='padding'>1</field>
            <field name='company_id' eval='False' />
        </record>
    </data>
</odoo>