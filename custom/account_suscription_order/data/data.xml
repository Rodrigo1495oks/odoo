<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate='1'> <!-- para que no se reinicie la secuencia-->
        <!-- sequences for shares_issuance -->
        <record id='seq_account_suscription_order' model='ir.sequence'>
            <field name='name'>Suscription Order</field>
            <field name='code'>account.suscription.order</field>
            <field name='prefix'>So</field>
            <field name='suffix'>R</field>
            <field name='padding'>12</field>
            <field name='company_id' eval='False' />
        </record>
        <!-- sequences for shares_issuance -->
        <record id='seq_account_suscription_order_stage' model='ir.sequence'>
            <field name='name'>Suscription Order Stage</field>
            <field name='code'>account.suscription.order.stage</field>
            <field name='prefix'>SoS</field>
            <field name='suffix'>S</field>
            <field name='padding'>6</field>
            <field name='company_id' eval='False' />
        </record>
    </data>
</odoo>