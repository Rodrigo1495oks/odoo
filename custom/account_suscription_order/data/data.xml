<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate='1'> <!-- para que no se reinicie la secuencia-->
        <!-- sequences for shares_issuance -->
        <record id='seq_assembly_meeting' model='ir.sequence'>
            <field name='name'>Assembly Meeting</field>
            <field name='code'>assembly.meeting</field>
            <field name='prefix'>AM</field>
            <field name='suffix'>F</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
        <!-- sequences for topic -->
        <record id='seq_assembly_meeting_topic' model='ir.sequence'>
            <field name='name'>Assembly Meeting Topic</field>
            <field name='code'>assembly.meeting.topic</field>
            <field name='prefix'>TP</field>
            <field name='suffix'>AM</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
        <!-- sequences for votes -->
        <record id='seq_assembly_meeting_vote' model='ir.sequence'>
            <field name='name'>Assembly Meeting Vote</field>
            <field name='code'>assembly.meeting.vote</field>
            <field name='prefix'>V</field>
            <field name='suffix'>T</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
    </data>
</odoo>