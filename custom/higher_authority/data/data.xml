<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate='1'> <!-- para que no se reinicie la secuencia-->
        <!-- sequences for suscription_order -->
        <record id='seq_suscription_order' model='ir.sequence'>
            <field name='name'>Suscription Order</field>
            <field name='code'>suscription.order</field>
            <field name='prefix'>SO</field>
            <field name='suffix'>ACC</field>
            <field name='padding'>1</field>
            <field name='company_id' eval='False' />
