<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate='1'> <!-- para que no se reinicie la secuencia-->
        <!-- sequences for account_share_cost -->
        <record id='seq_account_share_cost' model='ir.sequence'>
            <field name='name'>Account Share Cost</field>
            <field name='code'>account.share.cost</field>
            <field name='prefix'>ASC</field>
            <field name='suffix'>Sha</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
        <!-- sequences for account_share -->
        <record id='seq_account_share' model='ir.sequence'>
            <field name='name'>Account Share</field>
            <field name='code'>account.share</field>
            <field name='prefix'>AS</field>
            <field name='suffix'>S</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
        <!-- sequences for shares_issuance -->
        <record id='seq_account_share_issuance' model='ir.sequence'>
            <field name='name'>Account Share Issuance</field>
            <field name='code'>account.share.issuance</field>
            <field name='prefix'>SI</field>
            <field name='suffix'>Sh</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
        <!-- sequences for account_shrae_type -->
        <record id='seq_share_type' model='ir.sequence'>
            <field name='name'>Account Share Type</field>
            <field name='code'>account.share.type</field>
            <field name='prefix'>ST</field>
            <field name='suffix'>Sh</field>
            <field name='padding'>5</field>
            <field name='company_id' eval='False' />
        </record>
    </data>
</odoo>